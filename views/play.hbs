<script src="js/main.js"></script>
<h1>{{story.title}}</h1>
<div id="current">
	{{story.chapts.[0].current.effect}}
</div>
<form action="" method="POST">
	
	<input type="radio" name="choices" value="a" id="a"><label for='a'>{{story.chapts.[0].choiceA.action}}</label>
	<br>
	<input type="radio" name="choices" value="b" id="b"><label for='b'>{{story.chapts.[0].choiceB.action}}</label>
	<br>
	<input type="submit" name="btn" id="btn" value="Choose">
</form>
